baseURL = 'http://example.org/'
languageCode = 'en-us'
title = 'Test site for mod-popup'

[module]
  # Build and serve using local mod-popup clone declared in the named Hugo workspace:
  workspace = "mod-popup.work"
  replacements = "github.com/anoduck/mod-popup --> ../../"

  [[module.imports]]
    path = "github.com/anoduck/mod-popup"
  [[module.mounts]]
    source = "static"
    target = "static"
  [[module.imports.mounts]]
    source = 'dist/js/jquery.js'
    target = 'static/js/jquery.js'
  [[module.imports.mounts]]
    source = 'dist/js/autoload.js'
    target = 'static/js/autoload.js'
  [[module.imports.mounts]]
    source = "assets"
    target = "assets"
  [[module.imports.mounts]]
    source = 'layouts'
    target = 'layouts'